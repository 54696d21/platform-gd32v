{
    "name": "gd32v",
    "title": "Platform for GD32V Series MCU",
    "description": "GD32V is a Risc-V 32-bit MCU. This platform is ported by Sipeed.",
    "url": "https://www.sipeed.com",
    "homepage": "https://github.com/sipeed/platform-gd32v",
    "license": "Apache-2.0",
    "engines": {
        "platformio": "<5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sipeed/platform-gd32v.git"
    },
    "version": "0.0.1",
    "packageRepositories": [
        "http://dl.sipeed.com/LONGAN/platformio/dl-packages/manifest.json",
        "https://dl.bintray.com/platformio/dl-packages/manifest.json",
        "http://dl.platformio.org/packages/manifest.json"
    ],
    "frameworks": {
        "gd32vf103-sdk": {
            "package": "framework-gd32vf103-sdk",
            "script": "builder/frameworks/firmware_library.py"
        },
        "arduino": {
            "package": "framework-arduino-gd32v",
            "script": "builder/frameworks/arduino.py"
        }
    },
    "packages": {
        "framework-gd32vf103-sdk": {
            "optional": true,
            "type": "framework",
            "version": "~1.0.0"
        },
        "framework-arduino-gd32v": {
            "optional":true,
            "type": "framework",
            "version": "~0.1.1"
        },
        "tool-openocd-gd32v": {
            "optional": true,
            "type": "uploader",
            "version": "~0.1.1"
        },
        "tool-gd32vflash": {
            "type": "uploader",
            "optional": true,
            "version": "~0.1.0"
        },
        "toolchain-gd32v": {
            "type": "toolchain",
            "version": "~9.2.0"
        }
    }

}
